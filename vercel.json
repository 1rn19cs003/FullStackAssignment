{
    "version": 2,
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api-docs/(.*)",
            "dest": "/api-docs/index.html"
        },
        {
            "src": "/api-docs",
            "dest": "/api-docs/index.html"
        },
        {
            "src": "/(.*)",
            "dest": "app.js",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "PATCH",
                "DELETE"
            ]
        }
    ]
}
